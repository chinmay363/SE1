{
  "name": "apms-backend",
  "version": "1.0.0",
  "description": "Automated Parking Management System - Backend API",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest --coverage",
    "test:unit": "jest tests/unit --coverage",
    "test:integration": "jest tests/integration --coverage",
    "test:system": "jest tests/system --coverage",
    "test:setup": "node scripts/setup-test-db.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:test": "cross-env NODE_ENV=test sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "seed:test": "cross-env NODE_ENV=test sequelize-cli db:seed:all",
    "lint": "eslint src/ tests/"
  },
  "keywords": [
    "parking",
    "automation",
    "license-plate"
  ],
  "author": "APMS Team",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/app.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 35,
        "functions": 65,
        "lines": 62,
        "statements": 62
      }
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
